// create jenkinsfile with stages compile, test and package with echo commands

pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                echo 'Compiling..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.. webhooks'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging..'
            }
        }
    }
}