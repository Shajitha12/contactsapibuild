// jenkins pipeline script
// pipeline with stages compile, test, package and echo message in the stages
// checkinggit
pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                echo 'Compiling the code the code'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing the code'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging the code'
            }
        }
    }
}
